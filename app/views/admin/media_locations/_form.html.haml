= form_for @admin_media_location do |f|
  - if @admin_media_location.errors.any?
    #error_explanation
      %h2= "#{pluralize(@admin_media_location.errors.count, "error")} prohibited this admin_media_location from being saved:"
      %ul
        - @admin_media_location.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .actions
    = f.submit 'Save'
