<h1>Listing admin_retailers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Sector</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_retailers.each do |admin_retailer| %>
      <tr>
        <td><%= admin_retailer.name %></td>
        <td><%= admin_retailer.sector %></td>
        <td><%= link_to 'Show', admin_retailer %></td>
        <td><%= link_to 'Edit', edit_admin_retailer_path(admin_retailer) %></td>
        <td><%= link_to 'Destroy', admin_retailer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Retailer', new_admin_retailer_path %>
