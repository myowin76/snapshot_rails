<h1>Listing admin_store_formats</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_store_formats.each do |admin_store_format| %>
      <tr>
        <td><%= admin_store_format.name %></td>
        <td><%= link_to 'Show', admin_store_format %></td>
        <td><%= link_to 'Edit', edit_admin_store_format_path(admin_store_format) %></td>
        <td><%= link_to 'Destroy', admin_store_format, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Store format', new_admin_store_format_path %>
