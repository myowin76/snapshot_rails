<h1>Listing admin_countries</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_countries.each do |admin_country| %>
      <tr>
        <td><%= admin_country.name %></td>
        <td><%= link_to 'Show', admin_country %></td>
        <td><%= link_to 'Edit', edit_admin_country_path(admin_country) %></td>
        <td><%= link_to 'Destroy', admin_country, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Country', new_admin_country_path %>
