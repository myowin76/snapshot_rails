= form_for @admin_media_vehicle do |f|
  - if @admin_media_vehicle.errors.any?
    #error_explanation
      %h2= "#{pluralize(@admin_media_vehicle.errors.count, "error")} prohibited this admin_media_vehicle from being saved:"
      %ul
        - @admin_media_vehicle.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .actions
    = f.submit 'Save'
