= form_for @admin_media_type do |f|
  - if @admin_media_type.errors.any?
    #error_explanation
      %h2= "#{pluralize(@admin_media_type.errors.count, "error")} prohibited this admin_media_type from being saved:"
      %ul
        - @admin_media_type.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .actions
    = f.submit 'Save'
