<h1>Listing admin_brand_owners</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_brand_owners.each do |admin_brand_owner| %>
      <tr>
        <td><%= admin_brand_owner.name %></td>
        <td><%= link_to 'Show', admin_brand_owner %></td>
        <td><%= link_to 'Edit', edit_admin_brand_owner_path(admin_brand_owner) %></td>
        <td><%= link_to 'Destroy', admin_brand_owner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Brand owner', new_admin_brand_owner_path %>
